{
  "name": "express-websocket",
  "version": "1.0.0",
  "description": "Express with websocket",
  "main": "app.mjs",
  "scripts": {
    "http-server": "npx http-server -p 4000 ./src",
    "start": "node ./src/js/components/websocket-server.js",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "htmlhint": "npx htmlhint ./public || exit 0",
    "stylelint": "npx stylelint **/*.css || exit 0",
    "stylelint:fix": "npx stylelint --fix **/*.css || exit 0",
    "eslint": "npx eslint . || exit 0",
    "eslint:fix": "npx eslint --fix . || exit 0",
    "jsdoc": "npx jsdoc -c .jsdoc.json || exit 0",
    "lint": "npm run htmlhint && npm run stylelint && npm run eslint",
    "test": "npm run lint",
    "clean": "rm -rf build/",
    "clean-all": "npm run clean && rm -rf node_modules/ && rm -f package-lock.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.3",
    "ejs": "^3.1.9",
    "express": "^4.18.3",
    "express-session": "^1.18.0",
    "http": "^0.0.1-security",
    "morgan": "^1.10.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "htmlhint": "^1.1.4",
    "http-server": "^14.1.1",
    "jsdoc": "^4.0.2",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^13.0.0",
    "vite": "^5.0.2"
  }
}
